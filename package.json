{
  "name": "bedrock-manager",
  "version": "1.0.0",
  "description": "Web application for managing Minecraft Bedrock server.",
  "main": "index.js",
  "scripts": {
    "start": "node -r dotenv/config src/index.mjs",
    "start:watch": "nodemon --config nodemon.json --exec \"npm start\"",
    "test": "jest --config=jest.config.js",
    "prepare": "husky install",
    "format": "npx prettier",
    "lint": "npx eslint"
  },
  "keywords": [
    "minecraft",
    "bedrock",
    "server"
  ],
  "author": "Andrew Benz",
  "license": "ISC",
  "dependencies": {
    "express": "^4.19.2",
    "express-fileupload": "^1.5.0",
    "hbs": "^4.2.0",
    "module-alias": "^2.2.3",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@types/jest": "^29.5.12",
    "babel-jest": "^29.7.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^7.0.4",
    "jest": "^29.7.0",
    "jest-express": "^1.12.0",
    "lint-staged": "^12.5.0",
    "nodemon": "^3.1.4",
    "prettier": "^2.8.8"
  },
  "imports": {
    "##/*": "./src/*"
  }
}
